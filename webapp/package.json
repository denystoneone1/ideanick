{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "types": "tsc --noEmit -project tsconfig.app.json && tsc --noEmit -project tsconfig.node.json && tsc --noEmit -project tsconfig.json",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --no-source-map --load-path=node_modules/include-media/dist --load-path=node_modules --load-path=src --update ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level warn --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss}\""
  },
  "dependencies": {
    "@ideanick/backend": "workspace:*",
    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "include-media": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1",
    "reset-css": "^5.0.2",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "scss-reset": "^1.4.6",
    "stylelint": "^16.23.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
